{
    "AWSEBDockerrunVersion": "2",
    "containerDefinitions": [
        {
            "name": "client",
            "image": "satan555/multi-client",
            "hostname": "client",
            "essential": false
        },
        {
            "name": "server",
            "image": "satan555/multi-server",
            "hostname": "api",
            "essential": false
        },
        {
            "name": "worker",
            "image": "satan555/multi-worker",
            "hostname": "worker",
            "essential": false
        },
        {
            "name":"nginx",
            "image":"satan555/multi-nginx",
            "essential": true,
            "portMappings":[
                {
                    "hostPort": 80,
                    "containerPort": 80
                }
            ],
            "links": ["client","server"]
        }
    ]
}